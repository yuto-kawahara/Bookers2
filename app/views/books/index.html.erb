<%= render 'searches/search' %>
<%= render 'books/error', new_book: @new_book %>
<div class="row">
  <div class="col-md-3">
    <%= render 'books/new', new_book: @new_book, user: current_user %>
  </div>
  <div class="col-md-8 offset-md-1">
    <h2>Books</h2>
    <%= link_to "新着順", books_path(sort: "new_list") %> |
